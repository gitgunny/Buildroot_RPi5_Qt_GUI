  hostname $(hostname)

auto wlan0
iface wlan0 inet static
  address 192.168.219.199
  netmask 255.255.255.0
  gateway 192.168.219.1
  pre-up wpa_supplicant -B -i wlan0 -c /etc/wpa_supplicant.conf
  post-down killall -q wpa_supplicant
